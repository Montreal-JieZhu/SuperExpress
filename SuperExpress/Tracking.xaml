        xmlns:local="clr-namespace:SuperExpress"
        mc:Ignorable="d"
        Title="Tracking" Height="552" Width="929">
    <Grid Margin="0,0,2,4">
        <Label x:Name="label" Content="Tracking" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="50" Width="125" FontSize="24"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="15,65,0,0" VerticalAlignment="Top" Width="250" Height="35">
            <ComboBoxItem Content="UPS" IsSelected="True"/>
        </ComboBox>
        <ListView x:Name="listView" HorizontalAlignment="Left" Height="350" Margin="15,120,0,0" VerticalAlignment="Top" Width="250">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
